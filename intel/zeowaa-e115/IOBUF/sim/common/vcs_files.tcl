
namespace eval IOBUF {
  proc get_memory_files {QSYS_SIMDIR} {
    set memory_files [list]
    return $memory_files
  }
  
  proc get_common_design_files {QSYS_SIMDIR} {
    set design_files [dict create]
    return $design_files
  }
  
  proc get_design_files {QSYS_SIMDIR} {
    set design_files [dict create]
    dict set design_files "altera_gpio.sv"                   "$QSYS_SIMDIR/../altera_gpio_core14_191/sim/altera_gpio.sv"            
    dict set design_files "IOBUF_altera_gpio_1930_alkbnhy.v" "$QSYS_SIMDIR/../altera_gpio_1930/sim/IOBUF_altera_gpio_1930_alkbnhy.v"
    dict set design_files "IOBUF.v"                          "$QSYS_SIMDIR/IOBUF.v"                                                 
    return $design_files
  }
  
  proc get_elab_options {SIMULATOR_TOOL_BITNESS} {
    set ELAB_OPTIONS ""
    if ![ string match "bit_64" $SIMULATOR_TOOL_BITNESS ] {
    } else {
    }
    return $ELAB_OPTIONS
  }
  
  
  proc get_sim_options {SIMULATOR_TOOL_BITNESS} {
    set SIM_OPTIONS ""
    if ![ string match "bit_64" $SIMULATOR_TOOL_BITNESS ] {
    } else {
    }
    return $SIM_OPTIONS
